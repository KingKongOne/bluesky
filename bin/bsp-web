import tornado.ioloop

try:
    from bluesky.web.app import main
except:
    import os
    import sys
    root_dir = os.path.abspath(os.path.join(sys.path[0], '../'))
    sys.path.insert(0, root_dir)
    from bluesky.web.app import main

if __name__ == "__main__":
    main()
